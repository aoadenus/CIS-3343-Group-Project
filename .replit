modules = ["nodejs-20", "web", "postgresql-16", "python-3.11"]
[agent]
expertMode = true
integrations = ["javascript_stripe:1.0.0", "resend:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev-server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "backend-server"

[[workflows.workflow]]
name = "dev-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "backend-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run server:dev"
waitForPort = 3000

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34623
externalPort = 3002

[[ports]]
localPort = 35509
externalPort = 5173

[[ports]]
localPort = 36023
externalPort = 5000

[[ports]]
localPort = 39017
externalPort = 4200

[[ports]]
localPort = 41563
externalPort = 3003

[[ports]]
localPort = 42567
externalPort = 6000

[[ports]]
localPort = 45117
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "vite", "preview", "--host", "0.0.0.0", "--port"]
build = ["npm", "run", "build"]
